{
  "name": "zabaca-web",
  "type": "module",
  "version": "1.0.0",
  "description": "Zabaca company website monorepo built with Astro",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm --filter web build",
    "preview": "pnpm --filter web preview",
    "typecheck": "pnpm --filter web typecheck",
    "lint": "pnpm --filter web lint",
    "format": "pnpm --filter web format",
    "format:check": "pnpm --filter web format:check",
    "install:all": "pnpm install",
    "clean": "pnpm --filter web clean && rm -rf node_modules"
  },
  "keywords": [
    "astro",
    "react",
    "typescript",
    "tailwindcss",
    "monorepo",
    "pnpm-workspace"
  ],
  "author": "Zabaca",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}